load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "multitest_proto",
    srcs = [
        "multi1.proto",
        "multi2.proto",
        "multi3.proto",
    ],
    visibility = ["//visibility:public"],
    deps = ["//multi:multi_proto"],
)

go_proto_library(
    name = "multitest_go_proto",
    importpath = "github.com/golang/protobuf/protoc-gen-go/testdata/multi",
    proto = ":multitest_proto",
    visibility = ["//visibility:public"],
    deps = ["//vendor/multi:go_default_library"],
)

go_library(
    name = "go_default_library",
    embed = [":multitest_go_proto"],
    importpath = "github.com/golang/protobuf/protoc-gen-go/testdata/multi",
    visibility = ["//visibility:public"],
)
